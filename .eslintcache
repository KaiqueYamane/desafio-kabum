\n  useEffect(() => {\n    fetchData();\n  }, []);\n\n  async function fetchData() {\n    const { data, status } = await axios.get('/products');\n    if (status === 200) {\n      const _arraySliced = arraySliced(data, 4);\n      setProducts([..._arraySliced]);\n    } else\n      setProducts([]);\n  }\n\n  function arraySliced(base, maximo) {\n    let resultado = [[]];\n    let grupo = 0;\n\n    for (let indice = 0; indice < base.length; indice++) {\n      if (resultado[grupo] === undefined)\n        resultado[grupo] = [];\n\n      resultado[grupo].push(base[indice]);\n\n      if ((indice + 1) % maximo === 0)\n        grupo = grupo + 1;\n    }\n\n    return resultado;\n  }\n\n  return (\n    <Container>\n      <Row>\n        <Title>Lançamentos</Title>\n      </Row>\n\n      {\n        products.map(group =>\n          <ProductRow key={Math.random()}>\n            {\n              group.map(item =>\n                <ProductItem key={item.id}>\n                  <Product product={item} />\n                </ProductItem>\n              )\n            }\n          </ProductRow>\n        )\n      }\n\n    </Container>\n  );\n}","/home/cadmus/Documents/Kaique/github/desafio-kabum/src/store/modules/rootReducer.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Home/BannerCarousel/index.js",[],["174","175"],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Home/BannerCarousel/styled.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Home/Product/index.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Home/ProductList/styled.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Home/Product/styled.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/helpers/CurrencyHelper.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Cart/ProductCart/index.js",[],"/home/cadmus/Documents/Kaique/github/desafio-kabum/src/components/Cart/ProductCart/style